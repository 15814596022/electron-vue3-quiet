{
  "name": "electron-vue3-quiet",
  "version": "0.0.0",
  "description": "桌面端脚手架",
  "main": "./dist/main.js",
  "author": "安静 <528627554@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TaiAiAc/electron-vue3-quiet.git"
  },
  "bugs": {
    "url": "https://github.com/TaiAiAc/electron-vue3-quiet/issues"
  },
  "homepage": "https://github.com/TaiAiAc/electron-vue3-quiet#readme",
  "scripts": {
    "dev": "vite",
    "build:win64": "nr typecheck && vite build && esno scripts/builder.ts x64",
    "build:win32": "nr typecheck && vite build && esno scripts/builder.ts ia32",
    "build:mac": "nr typecheck && vite build && esno scripts/builder.ts arm64",
    "build:dir": "nr typecheck && vite build && esno scripts/builder.ts dir x64",
    "clear:build": "rimraf dist && rimraf out",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx --fix --ignore-path .eslintignore && prettier . --write --loglevel warn --ignore-path .eslintignore",
    "postinstall": "electron-builder install-app-deps",
    "prepare": "husky install",
    "analysis": "windicss-analysis",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:c8": "vitest run --coverage",
    "dep:upgrade": "yarn upgrade-interactive --latest",
    "commit": "git-cz",
    "commit-push":"git pull && git add . && git-cz && git push"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/eslint-parser": "^7.18.2",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@iconify/json": "^2.1.74",
    "@iconify/vue": "^3.2.1",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/ffi-napi": "^4.0.5",
    "@types/sqlite3": "^3.1.8",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vueuse/core": "^8.9.1",
    "c8": "^7.11.3",
    "chalk": "4.1.2",
    "colord": "^2.9.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.9.0",
    "dayjs": "^1.11.3",
    "dotenv": "^16.0.1",
    "electron": "^19.0.8",
    "electron-builder": "^23.0.3",
    "electron-devtools-vendor": "^1.1.0",
    "electron-log": "^4.4.7",
    "esbuild": "^0.14.43",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.2.0",
    "esno": "^0.16.3",
    "follow-redirects": "^1.15.1",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "javascript-obfuscator": "^4.0.0",
    "lint-staged": "^13.0.1",
    "naive-ui": "^2.31.0",
    "pinia": "^2.0.14",
    "portfinder": "^1.0.28",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.76.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "rollup-plugin-obfuscator": "^0.2.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.52.3",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3",
    "unplugin-auto-import": "^0.9.2",
    "unplugin-icons": "^0.14.3",
    "unplugin-vue-components": "0.21.1",
    "vfonts": "^0.0.3",
    "vite": "2.9.14",
    "vite-plugin-eslint": "^1.6.1",
    "vite-plugin-windicss": "^1.8.4",
    "vitest": "^0.17.1",
    "vue": "^3.2.37",
    "vue-eslint-parser": "^9.0.2",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.1.1",
    "vue-tsc": "^0.38.3",
    "windicss": "^3.5.4",
    "windicss-analysis": "^0.3.5"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "ffi-napi": "^4.0.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "sqlite3": "^5.0.8",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "electron",
    "vue3",
    "typescript",
    "vite",
    "rollup",
    "electron-builder",
    "typeorm"
  ]
}
